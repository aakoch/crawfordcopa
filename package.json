{
  "name": "crawfordcopa",
  "version": "1.0.0",
  "description": "",
  "dependencies": {
    "parcel": "^2.11.0",
    "pug": "^3.0.2"
  },
  "source": "src/index.html",
  "targets": [
    {
      "distDir": "dist"
    }
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "parcel build './src/*.html' --trace --no-scope-hoist --no-optimize --no-content-hash --no-source-maps --public-url ./",
    "watch": "parcel watch './src/*.html' --trace --no-scope-hoist --no-optimize --no-content-hash --no-source-maps --public-url ./",
    "start": "parcel serve -p 12345 './src/*.html' --trace --no-source-maps --public-url ./"
  },
